
<div class="new_projects_title">
	Add Project or Venture
</div>

<div class="new_project_form">
	<%= form_for @project do |f| %>
		<div class="row new_project_inner_form">
		    <form class="col s12">
		      <div class="row">
		        <div class="input-field col s6">
		          <%= f.text_field :name, :id => "name", :placeholder => "Project Name" %>
		          <!-- <label for="name">Project Name</label> -->
		        </div>
		        <div class="input-field col s6">
		          <%= f.text_field :website, :id => "website", :placeholder => "Website" %>
		          <!-- <label for="website">Website</label> -->
		        </div>
		      </div>
		      <div class="row">
		        <div class="input-field col s12">
		          <%= f.text_area :description, :class => "materialize-textarea", :id => "description", :placeholder => "Description"%>
		          <!-- <label for="description">Description</label> -->
		        </div>
		      </div>
		      <div class="row">
		        <div class="input-field col s6">
		          <%= f.text_field :image, :id => "image", :placeholder => "Image URL" %>
		          <!-- <label for="image">Image URL</label> -->
		        </div>
		        <div class="input-field col s6">
		          <%= f.text_field :contact, :id => "email", :type => "email", :class => "validate", :placeholder => "Email" %>
		          <!-- <label for="email" data-error="invalid email" data-success="yay">Email</label> -->
		        </div>
		      </div>
		      <div class="flex_button">
					<button class="btn-large waves-effect waves-light" type="submit" name="action">
						Submit
					</button>
				</div>
		    </form>
		</div>
	<% end %>
</div>





